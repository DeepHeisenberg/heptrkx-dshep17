  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "accuracy( bcnn.predict([test_data_sq,test_data_sq,test_data_sq]) )\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "cnn_4_dout = Sequential()\n",
    "cnn_4_dout.add(Convolution2D(10, 4, 4, input_shape = (1, 28, 28), activation='relu'))\n",
    "cnn_4_dout.add(MaxPooling2D())\n",
    "cnn_4_dout.add(Flatten())\n",
    "cnn_4_dout.add(Dense(1000, activation='relu'))\n",
    "cnn_4_dout.add(Dropout(0.5)) \n",
    "cnn_4_dout.add(Dense(N, init='uniform'))\n",
    "cnn_4_dout.add(Activation('softmax'))\n",
    "cnn_4_dout.compile(loss='mse', optimizer='sgd')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "hist_cnn_4_dout = cnn_4_dout.fit( train_data_sq, \n",
    "                  train_target_N,\n",
    "                  nb_epoch=1000, \n",
    "                  batch_size=100,\n",
    "                  verbose=0,\n",
    "                  validation_split=0.2,\n",
    "                  callbacks=[EarlyStopping(monitor='val_loss', patience=1, verbose=1, mode='auto'),\n",
    "                            ModelCheckpoint(filepath='cnn_4_dout.h5', verbose=0)]\n",
    "                 )"
   ]
