   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "def accuracy( p ):\n",
    "    p_cat = np.argmax(p,axis=1)\n",
    "    print \"Fraction of good prediction\"\n",
    "    print len(np.where( p_cat == test_target)[0])\n",
    "    print len(np.where( p_cat == test_target )[0])/float(len(p_cat)),\"%\"\n",
    "    \n",
    "def accuracy_m( model ):\n",
    "    ##figure out the shape of the input expected\n",
    "    if hasattr('input_dim', model.layers[0]):\n",
    "        p=model.predict(test_data)\n",
    "\n",
    "    accuracy(p)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": false
