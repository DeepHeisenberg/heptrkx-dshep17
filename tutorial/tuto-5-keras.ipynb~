    "bcnn_dout = Sequential()\n",
    "bcnn_dout.add(Merge([filter_5,filter_4,filter_3], mode='concat'))\n",
    "bcnn_dout.add(Dense(1000, activation='relu'))\n",
    "bcnn_dout.add(Dropout(0.5)) \n",
    "bcnn_dout.add(Dense(N, init='uniform'))\n",
    "bcnn_dout.add(Activation('softmax'))\n",
    "bcnn_dout.compile(loss='mse', optimizer='sgd')"
