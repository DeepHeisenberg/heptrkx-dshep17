   "source": [
    "Can we go even further ?"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 99,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "from keras.layers import Merge"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "Let's combine filters of size 3, 4 and 5"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "filter_5 = Sequential()\n",
    "filter_5.add(Convolution2D(10, 5, 5, input_shape = (1, 28, 28), activation='relu'))\n",
    "filter_5.add(MaxPooling2D())\n",
    "filter_5.add(Flatten())\n",
    "\n",
    "filter_4 = Sequential()\n",
    "filter_4.add(Convolution2D(10, 4, 4, input_shape = (1, 28, 28), activation='relu'))\n",
    "filter_4.add(MaxPooling2D())\n",
    "filter_4.add(Flatten())\n",
    "\n",
    "filter_3 = Sequential()\n",
    "filter_3.add(Convolution2D(10, 3, 3, input_shape = (1, 28, 28), activation='relu'))\n",
    "filter_3.add(MaxPooling2D())\n",
    "filter_3.add(Flatten())\n",
    "\n",
    "bcnn = Sequential()\n",
    "bcnn.add(Merge([filter_5,filter_4,filter_3], mode='concat'))\n",
    "bcnn.add(Dense(1000, activation='relu'))\n",
    "\n",
    "bcnn.add(Dense(N, init='uniform'))\n",
    "bcnn.add(Activation('softmax'))\n",
    "bcnn.compile(loss='mse', optimizer='sgd')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "hist_bcnn =bcnn.fit( [train_data_sq,train_data_sq,train_data_sq], \n",
    "                  train_target_N,\n",
    "                  nb_epoch=1000, \n",
    "                  batch_size=100,\n",
    "                  verbose=0,\n",
    "                  validation_split=0.2,\n",
    "                  callbacks=[EarlyStopping(monitor='val_loss', patience=1, verbose=1, mode='auto'),\n",
    "                            ModelCheckpoint(filepath='bcnn.h5', verbose=0)]\n",
    "                 )"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "show_losses( [\n",
    "              (\"hidden layer\", hist_deep),\n",
    "                (\"cnn filter 3\", hist_cnn_3),\n",
    "              (\"cnn filter 4\", hist_cnn_4),\n",
    "               (\"cnn filter 5\", hist_cnn_5),\n",
    "             (\"combined cnn\", hist_bcnn)\n",
    "            ] )"
   ]
