    "deeper_dout = Sequential()\n",
    "deeper_dout.add(Dense(1000, input_dim=28*28, activation='relu'))\n",
    "deeper_dout.add( Dropout(0.5) )\n",
    "deeper_dout.add(Dense(1000, init='uniform', activation='relu'))\n",
    "deeper_dout.add( Dropout(0.5) )\n",
    "deeper_dout.add(Dense(N, init='uniform'))\n",
    "deeper_dout.add(Activation('softmax'))\n",
    "deeper_dout.compile(loss='mse', optimizer='sgd')"
